{
  "extends": ["conventions", "prettier"],
  "plugins": ["prettier", "import", "unicorn"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": ["error", "always"],
    "unicorn/prevent-abbreviations": "error"
  }
}
